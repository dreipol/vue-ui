{
  "root": true,
  "parser": "babel-eslint",
  "extends": "@dreipol/eslint-config",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "Vue": true,
    "globals": true
  },
  "rules": {
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ]
  }
}
